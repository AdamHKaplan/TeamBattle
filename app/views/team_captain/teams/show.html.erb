<!-- show.html.erb -->
<div id="teammates">
  <ul>
  <%= @team.users.each do |u| %>
    <li><%= u.name %> <%= "(#{u.email})" %>)
      <ul>
        <li>HP: <%= u.hp %></li>
        <li>Attack: <%= u.attack_power %></li>
        <li>Heal: <%= u.heal_power %></li>
      </ul>
    </li>
  <% end %>
  </ul>
</div>